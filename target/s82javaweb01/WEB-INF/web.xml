<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <!--注册-->
  <servlet>
    <servlet-name>LoginViewServlet</servlet-name>
    <servlet-class>com.servlet.LoginViewServlet</servlet-class>
  </servlet>

  <!--映射-->
  <servlet-mapping>
    <servlet-name>LoginViewServlet</servlet-name>
    <url-pattern>/loginView.do</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>LoginActionServlet</servlet-name>
    <servlet-class>com.servlet.LoginActionServlet</servlet-class>
  </servlet>

  <!--映射-->
  <servlet-mapping>
    <servlet-name>LoginActionServlet</servlet-name>
    <url-pattern>com.servlet.RegistViewServlet</url-pattern>
  </servlet-mapping>



</web-app>
